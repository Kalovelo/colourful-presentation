{
  "plugins": [
    ["prismjs", {
        "languages": ["javascript", "css","ruby","haml", "markup","bash","shell-session"],
        "plugins": ["line-numbers","show-language"],
        "theme": "tomorrow",
        "css": true
    }]
  ],
  "presets": [
    [
      "babel-preset-gatsby",
      {
        "targets": {
          "browsers": [">0.25%", "not dead"]
        }
      }
    ]
  ]
}